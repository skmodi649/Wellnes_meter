<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.wellnesmeter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wellmet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wellmet"
        android:supportsRtl="true"
        android:theme="@style/Theme.WellnesMeter">
        <activity
            android:screenOrientation="portrait"
            android:name=".DetailActivity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".AffectedCountries"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".dashboard"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".Tabs_activity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".AboutUsActivity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".ResultActivity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".HealthIndexCalc"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".TermsAndConditions"
            android:exported="true"
            android:label="@string/title_activity_termsandcondtions"
            android:theme="@style/Theme.WellnesMeter.NoActionBar"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".InputActivity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true"
            android:label="@string/app_name"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterUser"
            android:label="@string/app_name" />
    </application>

</manifest>